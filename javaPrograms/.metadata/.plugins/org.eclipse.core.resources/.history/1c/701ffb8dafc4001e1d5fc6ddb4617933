package p1;

class ParentSuper {
    int num1;
    int num2;

    ParentSuper(int x, int y) {
        num1 = x;
        num2 = y;
        System.out.println("the sum: " + (num1 + num2));
    }
}

class ChildSuper extends ParentSuper {
    int num;

    ChildSuper(int x, int y, int z) {
        super(x, y);
        num = z;
    }
}


public class Supper {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
